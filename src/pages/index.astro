---
import Navbar from '../components/navbar.astro'
import Logocad from '../components/logo-CAD.astro'
import Hero from '../components/hero.astro'
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
	

		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>

	<style>
		body {
  margin: 8px; /* Márgenes predeterminados para el cuerpo del documento */
  font-family: initial; /* Fuente predeterminada del sistema */
  font-size: 16px; /* Tamaño de fuente predeterminado */
  line-height: 1.5; /* Altura de línea predeterminada */
  color: initial; /* Color de texto predeterminado */
  background-color: initial; /* Color de fondo predeterminado */
}

	</style>
	<body>
		<Navbar></Navbar>
		<main>
			<Logocad></Logocad>
			<Hero></Hero>
		</main>
	

		
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
	</body>
</html>
